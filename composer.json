{
    "name": "yorus/behat-context",
    "description": "Provide some useful behat contexts",
    "keywords": ["behat", "contexts", "orm", "amqp", "fidry-alice", "rest", "doctrine"],
    "license": "MIT",
    "authors": [{
        "name": "Yoann ROUSSEAU",
        "homepage": "https://github.com/YoRusConsulting/behat-context"
    }],
    "autoload": {
        "psr-4": {
            "YoRus\\BehatContext\\": "src"
        }
    },
    "require": {
        "php": ">=8.0",
        "behat/behat": "~3.0",
        "doctrine/orm": "^3.1",
        "theofidry/alice-data-fixtures": "^1.7",
        "symfony/amqp-messenger": "^5.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.10",
        "ubirak/rest-api-behat-extension": "^8.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
